<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edittext1demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EnableDisableActivity"
            android:label="@string/title_activity_enable_disable" >
        </activity>
        <activity
            android:name=".MaxLengthActivity"
            android:label="@string/title_activity_max_length" >
        </activity>
        <activity
            android:name=".ValidationActivity"
            android:label="@string/title_activity_validation" >
        </activity>
        <activity
            android:name=".DynamicActivity"
            android:label="@string/title_activity_dynamic" >
        </activity>
    </application>

</manifest>
